$ ->
  $('#new_user').on('ajax:success', (e, data, status, xhr) ->
    if data.status == 'success'
      Turbolinks.visit('/')
    else
      notify 'warning', data.message
  ).on 'ajax:error', (e, xhr, status, error) ->
    notify 'danger', '<strong>An unexpected error occurred.</strong> ' + error
